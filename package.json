{
  "name": "news-glance",
  "version": "0.2.0",
  "private": true,
  "homepage": "http://akshay-sarkar.github.io/news-glance",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "firebase": "^11.6.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.0",
    "react-burger-menu": "^3.1.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.4.0",
    "react-scripts": "5.0.1",
    "react-swipeable-views": "^0.14.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "deploy:manual": "./scripts/deploy.sh manual",
    "deploy:ci-cd": "./scripts/deploy.sh ci-cd",
    "deploy:promote": "./scripts/deploy.sh ci-cd",
    "test:ci": "react-scripts test --ci --watchAll=false --passWithNoTests",
    "build:analyze": "npm run build && npx serve -s build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "edge >= 18"
    ],
    "production": [
      "> 0.2%",
      "not dead"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}
